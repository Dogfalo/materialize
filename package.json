{
  "name": "materialize",
  "description": "Seat Engine Materialize",
  "private": true,
  "author": "Seat Engine",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/SeatEngine/materialize.git"
  },
  "scripts": {
    "start": "brunch watch --server",
    "build": "mkdir -p lib && npm run build:components && npm run build:ui",
    "build:components": "babel app/js/all.js > lib/all.js && babel app/js/components -d lib/components",
    "build:ui": "babel app/js/ui.js > lib/ui.js && babel app/js/ui -d lib/ui"
  },
  "main": "./lib/all.js",
  "dependencies": {
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2",
    "autoprefixer": "^7.1.1",
    "babel-brunch": "^6.1.1",
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "brunch": "^2",
    "clean-css-brunch": "^2.10.0",
    "isdev": "^1.0.1",
    "javascript-brunch": "^2.10.0",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "sass-brunch": "^2.10.4",
    "uglify-js-brunch": "^2.10.0"
  }
}
